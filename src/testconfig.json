{
  "platform": {
    "execution": {
      "parallel": true
    }
  },
  "extensions": [
    {
      "extensionId": "Microsoft.Testing.Extensions.CodeCoverage",
      "settings": {
        "format": "cobertura",
        "skipAutoProperties": true,
        "excludeAssemblyFilters": [
          "LoginApp*",
          "AvaloniaApplication1*",
          "ReactiveUI.Validation.Tests*"
        ]
      }
    }
  ]
}
